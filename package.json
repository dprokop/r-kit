{
  "name": "redux-kit",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "karma start config/karma.config.js",
    "test:watch": "karma start config/karma.config.js --no-single-run --auto-watch",
    "dev": "rm -rf ./dist && export NODE_ENV=development && webpack --config config/webpack.config.js",
    "dev:watch": "export NODE_ENV=development && webpack -d --watch --display-error-details",
    "dist": "rm -rf ./dist && export NODE_ENV=production && webpack --config config/webpack.prod.config.js",
    "serve": "export NODE_ENV=development && webpack-dev-server --inline --hot --open --content-base ./ --config config/webpack.config.js",
    "docs": "jsdoc --configure config/jsdoc.config.json --verbose"
  },
  "author": "Dominik Prokop",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^3.0.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "redux": "^3.1.6"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "expose-loader": "^0.7.1",
    "jasmine": "^2.4.1",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.19",
